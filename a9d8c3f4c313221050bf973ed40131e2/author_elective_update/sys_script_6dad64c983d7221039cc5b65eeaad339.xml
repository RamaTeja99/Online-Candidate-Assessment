        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {
    // Score dictionaries - all lowercase for easier matching
    var jobRoleScores = {
        "software developer": 10,
        "data scientist": 9,
        "cyber analyst": 8,
        "backend developer": 7
    };
    var skillsScores = {
        "none": 0,
        "java": 10,
        "python": 9,
        "react": 9,
        "javascript": 8
    };
    var qualificationScores = {
        "mtech": 10,
        "btech": 9,
        "bca": 7,
        "bba": 8
    };

    var score = 0;

    // Job role score
    var jobRole = (current.job_role_applied + "").trim().toLowerCase();
    if (jobRole && jobRoleScores.hasOwnProperty(jobRole)) {
        score += jobRoleScores[jobRole];
    }

    // Skills score (Glide List handling)
    var skillsStr = (current.skills + "").trim();
    if (skillsStr) {
        var skillIds = skillsStr.split(',');
        for (var i = 0; i < skillIds.length; i++) {
            var skillGR = new GlideRecord("u_skills"); // reference table name
            if (skillGR.get(skillIds[i])) {
                var skillName = (skillGR.getDisplayValue("name") + "").trim().toLowerCase();
                if (skillsScores.hasOwnProperty(skillName)) {
                    score += skillsScores[skillName];
                }
            }
        }
    }

    // Qualification score
    var qualification = (current.qualification + "").trim().toLowerCase();
    if (qualification && qualificationScores.hasOwnProperty(qualification)) {
        score += qualificationScores[qualification];
    }

    // Update or insert in admin_data table
    var adminGR = new GlideRecord('x_1815655_online_0_admin_data'); 
    adminGR.addQuery('candidate', current.sys_id);
    adminGR.query();

    if (adminGR.next()) {
        adminGR.assessment_score = score;
        adminGR.update();
    } else {
        adminGR.initialize();
        adminGR.number = gs.generateGUID();
        adminGR.assessment_score = score;
        adminGR.candidate = current.sys_id;
        adminGR.insert();
    }

})(current, previous);
]]></script>
