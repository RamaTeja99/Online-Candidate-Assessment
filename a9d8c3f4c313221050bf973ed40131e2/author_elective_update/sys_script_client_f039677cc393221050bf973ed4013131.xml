        <name>Skill Score</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading) {
    if (isLoading || newValue === '') {
        return;
    }

    // Scores mapping
    var jobRoleScores = {
        "Software Developer": 10,
        "Data Scientist": 9,
        "Cyber Analyst": 8,
        "Backend Developer": 7
    };

    var skillScores = {
        "None": 0,
        "Java": 10,
        "Python": 9,
        "React": 9,
        "JavaScript": 8
    };

    var qualificationScores = {
        "MTech": 10,
        "BTech": 9,
        "BCA": 7,
        "BBA": 8
    };

    // Get values from form
    var jobRole = g_form.getValue("job_role_applied");
    var skills = g_form.getValue("skills"); // comma-separated string for list type
    var qualification = g_form.getValue("qualification");

    // Calculate score
    var totalScore = 0;

    if (jobRole && jobRoleScores[jobRole]) {
        totalScore += jobRoleScores[jobRole];
    }

    if (skills) {
        var skillList = skills.split(",");
        for (var i = 0; i < skillList.length; i++) {
            var skillTrimmed = skillList[i].trim();
            if (skillScores[skillTrimmed]) {
                totalScore += skillScores[skillTrimmed];
            }
        }
    }

    if (qualification && qualificationScores[qualification]) {
        totalScore += qualificationScores[qualification];
    }

    // Set value to assessment_score field in Admin table ref
    g_form.setValue("assessment_score", totalScore);
}
]]></script>
