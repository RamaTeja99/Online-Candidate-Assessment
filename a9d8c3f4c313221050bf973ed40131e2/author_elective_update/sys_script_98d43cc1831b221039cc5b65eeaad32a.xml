        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {

    // Get the Candidate record
    var cand = current.candidate.getRefRecord();
    if (!cand || !cand.isValidRecord()) {
        return;
    }

    var score = 0;

    // --- Job Role Scores ---
    var jobRoleScores = {
        "Software Developer": 10,
        "Data Scientist": 9,
        "Cyber Analyst": 8,
        "Backend Developer": 7
    };

    if (cand.job_role_applied) {
        score += jobRoleScores[cand.job_role_applied.toString()] || 0;
    }

    // --- Qualification Scores ---
    var qualificationScores = {
        "mtech": 10,
        "btech": 9,
        "bca": 7,
        "bba": 8
    };

    if (cand.qualification) {
        score += qualificationScores[cand.qualification.toString().toLowerCase()] || 0;
    }

    // --- Skills Scores (List field) ---
    var skillsScores = {
        "none": 0,
        "java": 10,
        "python": 9,
        "react": 9,
        "javascript": 8
    };

    if (cand.skills) {
        var skillsArray = cand.skills.toString().split(','); // List field stored as comma-separated
        skillsArray.forEach(function(skill) {
            var trimmedSkill = skill.trim().toLowerCase();
            score += skillsScores[trimmedSkill] || 0;
        });
    }

    // Store final score in Admin table's assessment_score field
    current.assessment_score = score;

})(current, previous);
]]></script>
